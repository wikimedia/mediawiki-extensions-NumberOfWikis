{
	"name": "Number of wikis",
	"version": "0.4",
	"author": [
		"Jack Phoenix"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:NumberOfWikis",
	"description": "Adds <nowiki>{{NUMBEROFWIKIS}}</nowiki> magic word to show the number of wikis on ShoutWiki",
	"type": "variable",
	"ExtensionMessagesFiles": {
		"NumberOfWikisMagic": "NumberOfWikis.i18n.magic.php"
	},
	"AutoloadClasses": {
		"NumberOfWikis": "NumberOfWikis.class.php"
	},
	"Hooks": {
		"ParserGetVariableValueSwitch": [
			"NumberOfWikis::assignValue"
		],
		"MagicWordwgVariableIDs": [
			"NumberOfWikis::variableIds"
		]
	},
	"manifest_version": 1
}
